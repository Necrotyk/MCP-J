{
    "max_memory_mb": 512,
    "max_cpu_quota_pct": 100,
    "allowed_egress_ips": [
        "127.0.0.1"
    ],
    "readonly_mounts": [
        "/bin",
        "/usr/bin",
        "/lib",
        "/usr/lib",
        "/lib64",
        "/usr/lib64",
        "/usr/lib/python3.10",
        "/usr/local/lib/python3.10",
        "/etc/alternatives"
    ],
    "env_vars": {
        "PYTHONPATH": "/workspace",
        "PYTHONUNBUFFERED": "1"
    }
}